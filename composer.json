{
    "name": "bluenty/export-product",
    "description": "A Magento 2 module for exporting product data asynchronously.",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "php": "^7.3|^7.4|^8.1",
        "magento/framework": "^103.0",
        "magento/module-catalog": "^104.0",
        "magento/module-ui": "^102.0",
        "magento/module-backend": "^102.0"
    },
    "suggest": {
        "magento/module-cron": "Allows the module to handle scheduled export tasks"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Bluenty\\ExportProduct\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "Bluenty/ExportProduct"
            ]
        ]
    }
}
